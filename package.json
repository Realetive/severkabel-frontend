    "pobem": "^1.1.2",
    "postcss": "^6.0.23",
    "postcss-each": "^0.10.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^11.1.0",
    "postcss-inline-svg": "^3.1.1",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-reporter": "^5.0.0",
    "postcss-simple-vars": "^4.1.0",
    "postcss-url": "^7.3.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "pretty-error": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "tiny-lr": "^1.1.1",
