{
  "name": "severcabel",
  "version": "0.0.1",
  "description": "Frontend side for Sever Cabel site",
  "keywords": [],
  "author": "Roman @Realetive Ganin",
  "private": true,
  "homepage": "https://github.com/realetive/severcabel#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/realetive/severcabel.git"
  },
  "scripts": {
    "make": "node -r dotenv/config node_modules/enb/bin/enb make",
    "__postinstall": "npm run drop-cache && npm run make",
    "start": "node -r dotenv/config server",
    "dev": "npm run drop-cache && npm run make && nodemon",
    "drop-cache": "rm -rf .enb/tmp",
    "prod": "npm run drop-cache && npm run make",
    "watch": "chokidar 'components/**' --initial -c 'enb make && notify -t $HOST -m Built'",
    "lint": "eslint --fix . && bemhint .",
    "test": "npm run lint",
    "share": "ngrok http 3012"
  },
  "pre-commit": [
    "test"
  ],
  "engines": {
    "node": ">=10"
  },
  "browserslist": [
    "last 1 versions",
    "iOS 7"
  ],
  "dependencies": {
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/client": "^0.141.5",
    "@sanity/image-url": "^0.140.12",
    "akismet": "^2.0.5",
    "bem-components": "^6.0.1",
    "bem-core": "github:Realetive/bem-core#feature/#1269-partial-svg-support",
    "bem-forms": "github:realetive/bem-forms",
    "bem-history": "^4.0.0",
    "date-fns": "^2.0.0-beta.4",
    "entities": "^2.0.0",
    "nodemailer": "^6.3.0",
    "photoswipe": "^4.1.3",
    "postcss-csso": "^4.0.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "axios-extensions": "^2.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bemhint": "^0.10.1",
    "bemhint-css-naming": "^1.0.1",
    "bemhint-deps-specification": "^1.0.0",
    "bemhint-fs-naming": "^1.0.0",
    "body-parser": "^1.18.2",
    "borschik": "github:Realetive/borschik#master",
    "chokidar": "^2.0.3",
    "compression": "^1.7.2",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "^1.4.3",
    "debug-http": "^1.1.0",
    "dotenv": "^5.0.1",
