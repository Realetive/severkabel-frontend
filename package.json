  },
  "optionalDependencies": {
    "bem-tools-core": "^0.0.3",
    "bem-tools-create": "^2.2.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": false,
    "execMap": {
      "js": "node"
    },
    "watch": [
      "server",
      ".enb"
    ],
    "ext": "js"
  }
}
